{
    "name": "legwork-online",
    "version": "1.0.0",
    "description": "Online tool for using LEGWORK",
    "main": "main.js",
    "scripts": {
        "build": "npm run css",
        "css-compile": "node-sass --include-path node_modules --source-map true --output-style compressed --source-map-contents true --precision 6 scss -o css/",
        "css-lint": "stylelint scss/",
        "css-prefix": "postcss --replace css/main.css --use autoprefixer --map",
        "css-purge": "purgecss --keyframes --css css/main.css --content html/main.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output css/",
        "css": "npm-run-all css-compile css-prefix",
        "server": "serve --listen 3000",
        "start": "npm-run-all --parallel watch server",
        "watch": "nodemon -e html,scss -x \"npm run css\"",
        "test": "npm run css-lint && npm run css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TeamLEGWORK/LEGWORK-online.git"
    },
    "author": "Tom Wagg",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TeamLEGWORK/LEGWORK-online/issues"
    },
    "homepage": "https://github.com/TeamLEGWORK/LEGWORK-online#readme",
    "dependencies": {
        "bootstrap": "^5.2.0-beta1",
        "bootstrap-icons": "^1.4.0",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1"
    },
    "devDependencies": {
        "autoprefixer": "^10.2.5",
        "node-sass": "^7.0.0",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.10",
        "postcss-cli": "^8.3.1",
        "purgecss": "^4.0.2",
        "serve": "^11.3.2",
        "stylelint": "^13.12.0",
        "stylelint-config-twbs-bootstrap": "^2.1.0"
    },
    "browserslist": [
        ">= 0.5%",
        "last 2 major versions",
        "not dead",
        "Chrome >= 60",
        "Firefox >= 60",
        "Firefox ESR",
        "iOS >= 12",
        "Safari >= 12",
        "not Explorer <= 11"
    ]
}
